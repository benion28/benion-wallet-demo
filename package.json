{
  "name": "benion-wallet-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:client": "next dev",
    "dev:server": "tsnd --respawn --transpile-only --project tsconfig.json server.ts",
    "dev": "concurrently \"npm run dev:server\" \"wait-on http://localhost:8822 && npm run dev:client\"",
    "build:client": "next build",
    "build:server": "tsc -p tsconfig.json",
    "build": "npm run build:client && npm run build:server",
    "start:client": "next start",
    "start:server": "node dist/server.js",
    "start": "concurrently \"npm run start:server\" \"wait-on http://localhost:8822 && npm run start:client\"",
    "lint": "next lint",
    "server": "tsnd --respawn --transpile-only --project tsconfig.json server.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "mongoose": "^8.15.1",
    "next": "15.3.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  }
}
